[alias]
	st = status
	ci = commit
  ca = commit --all
	co = checkout
	br = branch
  dc = diff --staged
  tree = log --graph --decorate --pretty=oneline --abbrev-commit
  l = log --pretty=oneline --abbrev-commit
  new = log ORIG_HEAD.. --stat --no-merges
[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
